{
  "extends": ["eslint:recommended","plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "env": {
    "node":true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": ["warn",{
      "args":"none"
    }],
    "no-console": 1,
    "no-unexpected-multiline": "warn",
    "react/prop-types": [1,{"skipUndeclared":true}]
  }
}