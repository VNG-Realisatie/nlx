{
  "name": "@nlxio/management-ui-e2e-tests",
  "version": "0.0.0",
  "scripts": {
    "pretest:browserstack": "node outputUrl.js",
    "pretest:local": "node outputUrl.js",
    "pretest": "rimraf test-results",
    "test": "if test -n \"$CI\"; then npm run test:browserstack:edge; else npm run test:local; fi",
    "test:browserstack:edge": "testcafe 'browserstack:edge' --reporter 'spec,html:./test-results/index.html'",
    "test:browserstack:all-except-edge": "testcafe 'browserstack:firefox,browserstack:chrome,browserstack:safari' --reporter 'spec,html:./test-results/index.html'",
    "test:browserstack:all": "testcafe 'browserstack:edge,browserstack:firefox,browserstack:chrome,browserstack:safari' --quarantine-mode --reporter 'spec,html:./test-results/index.html'",
    "test:local": "testcafe 'firefox:headless' --live --reporter 'spec,html:./test-results/index.html'",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@commonground/eslint-config": "4.1.0",
    "axe-core": "4.1.1",
    "axe-testcafe": "3.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-header": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "prettier": "2.2.0",
    "rimraf": "3.0.2",
    "testcafe": "1.9.4",
    "testcafe-browser-provider-browserstack": "1.13.1",
    "testcafe-react-selectors": "4.1.1",
    "testcafe-reporter-html": "1.4.6"
  }
}
