{
  "extends": [
    "config:base",
    ":semanticCommits",
    "schedule:nonOfficeHours",
    "group:allNonMajor"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "ignoreDeps": [
    "github.com/coreos/etcd",
    "redoc"
  ],
  "masterIssue": true,
  "gomod": {
    "major": {
      "masterIssueApproval": true
    }
  }
}