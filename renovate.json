{
  "extends": [
    "config:base",
    ":semanticCommits",
    ":semanticCommitTypeAll(build)",
    ":semanticCommitScopeDisabled",
    "schedule:nonOfficeHours"
  ],
  "automerge": true,
  "automergeType": "pr",
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "ignoreDeps": [
    "github.com/coreos/etcd"
  ],
  "masterIssue": true,
  "gomod": {
    "major": {
      "masterIssueApproval": true
    }
  },
  "packageRules": [
    {
      "packageNames": ["history"],
      "manager": ["npm"],
      "allowedVersions": "<5.0.0"
    },
    {
      "groupName": "etcd fails with grpc > 1.26.0",
      "managers": ["gomod"],
      "packageNames": ["google.golang.org/grpc"],
      "allowedVersions": "<=1.26.0"
    },
    {
      "groupName": "genproto updates grpc > 1.26.0",
      "managers": ["gomod"],
      "packageNames": ["google.golang.org/genproto"],
      "allowedVersions": "0.0.0-20200128133413-58ce757ed39b"
    }
  ]
}
